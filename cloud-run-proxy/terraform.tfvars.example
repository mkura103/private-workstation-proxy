# Cloud Run Proxy Server - Terraform Configuration Example
# Copy this file to terraform.tfvars and fill in your values

# Required: GCP Project ID
project_id = "your-project-id"

# Required: Workstation cluster hostname
# Get this from workstations terraform output: cluster_hostname
cluster_hostname = "cluster-xxx.cloudworkstations.dev"

# Optional: Region (default: asia-northeast1)
# region = "asia-northeast1"

# Optional: Cloud Run service name (default: workstation-proxy)
# service_name = "workstation-proxy"

# Optional: VPC network name (default: default)
# network = "default"

# Optional: Subnet name (default: default)
# subnet = "default"

# Optional: Authentication mode (default: password)
# "password" - Simple password authentication (for non-organization projects)
# "iap" - IAP authentication (for organization projects)
# auth_mode = "password"

# Optional: Proxy password (required when auth_mode is "password")
# proxy_password = "your-secure-password"
